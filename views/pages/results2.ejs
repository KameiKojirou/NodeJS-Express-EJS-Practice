<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>
  <div class="jumbotron">
    <h1>This is great</h1>
    <p>Welcome to templating using EJS</p>
    <% results.forEach(function(results) { %>
        <li>
          <strong>Num: <%= results.num %></strong>,
          <strong>First: <%= results.name %></strong>,
          <strong>Last: <%= results.last %></strong>,
        </li>
    <% }); %>
  </div>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>